@article{Ferreira_Lopes_Oliveira_Carvalho_Souza_Santos_Silva_Veloso_2024,
  title        = {O uso de tecnologias digitais e plataformas de ensino como estratégias de ensino-aprendizagem em cursos de licenciatura em Educação Física e Pedagogia em uma universidade pública na cidade de Januária/MG},
  volume       = {21},
  url          = {https://ojs.studiespublicacoes.com.br/ojs/index.php/cadped/article/view/8594},
  doi          = {10.54033/cadpedv21n10-030},
  abstractnote = {&amp;lt;p class=&amp;quot;Citao&amp;quot; style=&amp;quot;margin: 0cm; margin-bottom: .0001pt;&amp;quot;&amp;gt;&amp;lt;span lang=&amp;quot;PT&amp;quot; style=&amp;quot;font-family: ’Arial’,sans-serif;&amp;quot;&amp;gt;Este estudo analisou a adoção de tecnologias digitais e plataformas de ensino nos cursos de licenciatura em Educação Física e Pedagogia de uma universidade pública em Januária/MG. O levantamento mostrou que ferramentas como Google Classroom e Google Meet são amplamente utilizadas e bem avaliadas pelos alunos, proporcionando vantagens como o acesso facilitado a conteúdos e a comunicação eficiente entre docentes e discentes. Entretanto, a pesquisa também revelou obstáculos importantes, tais como dificuldades de acesso à internet e a carência de suporte técnico e treinamento adequado para os professores. As percepções dos estudantes indicaram uma visão predominantemente favorável à eficácia das plataformas digitais, embora tenham surgido pontos de melhoria. Entre as áreas de maior destaque está a necessidade de investimentos contínuos em infraestrutura tecnológica e capacitação docente, a fim de aprimorar o uso dessas ferramentas no ambiente educacional. Ao abordar esses aspectos, o estudo contribui para uma melhor compreensão dos desafios e oportunidades da educação superior no contexto da transformação digital, reforçando a relevância de políticas educacionais que apoiem a integração efetiva das tecnologias no processo de ensino-aprendizagem.&amp;lt;/span&amp;gt;&amp;lt;/p&amp;gt;},
  number       = {10},
  journal      = {Caderno Pedagógico},
  author       = {Ferreira, Camila Ribeiro and Lopes, Érika Lucas and Oliveira, Júnea Tatiane Damasceno and Carvalho, Larissa Betânia Lacerda Araújo de and Souza, Maria Thereza Santos Ferreira e and Santos, Nathany Gonçalves and Silva, Naura Sthocco and Veloso, Rossane Glads Freitas Pereira},
  year         = {2024},
  month        = {out.},
  pages        = {e8594}
}

